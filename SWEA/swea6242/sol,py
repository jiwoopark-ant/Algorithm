import sys
sys.stdin = open ('input.txt',encoding='utf-8')



# blood_type=['A', 'A', 'A', 'O', 'B', 'B', 'O', 'AB', 'AB', 'O']

# for i in blood_type:


a=['A', 'A', 'A', 'O', 'B', 'B', 'O', 'AB', 'AB', 'O']
dic = {}

for i in a:
    #이미 기록된경우
    if i in dic.keys():
        dic[i] +=1
    # 처음 등장한 경우
    else:
        dic[i] =1
print(dic)    